<h2><%= t('form.lizenzo_importer.heading') %></h2>

<%= render "shared/error_messages", :target => @lizenzo_import %>
<%= form_for([:admin, @lizenzo_import], :html => { :multipart => true }) do |f| %>
<fieldset>
  <%= f.field_container :data_file do %>
    <%= f.label :data_file, t('form.lizenzo_importer.new.data_file') %>
    <%= f.file_field :data_file %>
    <%= f.error_message_on :data_file %>
  <% end %>
</fieldset>

<p class="form-buttons">
  <%= button t("actions.create") %>
</p>

<% end %>
